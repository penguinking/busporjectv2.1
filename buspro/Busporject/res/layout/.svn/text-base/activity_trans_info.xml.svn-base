<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TransFragment" >

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_above="@id/main_ads"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/linearLayout1"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:background="@color/listviewselected" >

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/imageView1"
            android:layout_marginTop="10dp"
            android:layout_toLeftOf="@+id/imageView3"
            android:layout_toRightOf="@+id/imageView1"
            android:src="@drawable/jiantou" />

          <ImageView
              android:id="@+id/imageView5"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignLeft="@+id/imageView1"
              android:layout_marginLeft="5dp"
              android:layout_below="@+id/imageView1"
              android:src="@drawable/line" />

          <ImageView
              android:id="@+id/imageView3"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignLeft="@+id/imageView1"
              android:layout_below="@+id/imageView5"              
              android:src="@drawable/img_change_transfer" />

          <ImageView
              android:id="@+id/imageView2"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignLeft="@+id/imageView3"
              android:layout_below="@+id/imageView6"
              
              android:src="@drawable/img_change_dest" />

          <ImageView
              android:id="@+id/imageView6"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignLeft="@+id/imageView3"
              android:layout_below="@+id/imageView3"
              android:layout_marginLeft="5dp"
              android:src="@drawable/line" />

          <TextView
              android:id="@+id/txt_zd1"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignTop="@+id/imageView1"
              android:layout_marginLeft="28dp"
              android:layout_marginTop="3dp"
              android:layout_toRightOf="@+id/imageView1"
              android:text="TextView"
              android:textColor="@color/black"
              android:textSize="20dp" />

          <TextView
              android:id="@+id/txt_zd2"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignLeft="@+id/txt_zd1"
              android:layout_alignTop="@+id/imageView3"
              android:layout_marginTop="3dp"
              android:text="TextView"
              android:textColor="@color/black"
              android:textSize="20dp" />

          <TextView
              android:id="@+id/txt_zd3"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignLeft="@+id/txt_zd2"
              android:layout_alignTop="@+id/imageView2"
              android:layout_marginTop="3dp"
              android:text="TextView"
              android:textColor="@color/black"
              android:textSize="20dp" />

          <ImageView
              android:id="@+id/imageView1"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignParentLeft="true"
              android:layout_alignParentTop="true"
              android:layout_marginLeft="56dp"
              android:layout_marginTop="16dp"
              android:src="@drawable/img_change_start" />

          <TextView
              android:id="@+id/txt_xl1"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
			  android:layout_toRightOf="@id/imageView5"
              android:layout_alignTop="@+id/imageView5"
              android:layout_marginTop="22dp"
              android:layout_marginLeft="10dp"
              android:text="TextView"
              android:textColor="@color/yellow" />

          <TextView
              android:id="@+id/txt_xl2"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_toRightOf="@id/imageView3"
              android:layout_alignTop="@+id/imageView6"
              android:layout_marginTop="26dp"
              android:text="TextView"
              android:layout_marginLeft="10dp"
              android:textColor="@color/yellow" />

    </RelativeLayout>
    

</RelativeLayout>